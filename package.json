    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start swc",
    "start:dev": "nest start -b swc -w --type-check",
    "start:debug": "nest start --debug --watch | pino-pretty",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config src/__tests__/jest-e2e.json",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/config/database/data-source.ts",
    "migration:create": "typeorm-ts-node-commonjs migration:create src/migrations/",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/config/database/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/config/database/data-source.ts",
    "seed": "ts-node -r tsconfig-paths/register src/config/database/seeds/seed.ts"
  },
